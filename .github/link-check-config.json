{
  "projectBaseUrl": "docs",
  "ignorePatterns": [
    {
      "pattern": "(localhost)"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "(^\\/(architecture|event)[^#]*)(#.*|$)",
      "replacement": "$1.md$3"
    },
    {
      "pattern": "^(\\/|@site\\/)",
      "replacement": "{{BASEURL}}/docs/"
    }
  ],
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "30s",
  "timeout": "30s"
}
